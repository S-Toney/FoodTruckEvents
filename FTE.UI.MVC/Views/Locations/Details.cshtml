@model FTE.DATA.EF.Location

@{
    ViewBag.Title = "Location";
}
<section id="contact" class="section-bg">
    <div class="container" data-aos="fade-up">
        <div class="section-header">
            <p>@ViewBag.Title</p>
            <h2>@Model.LocationName</h2>
            <p class="text-center">
                <a href="@Url.Action("Edit", "Locations", new { id = Model.LocationID })" class="mx-3"><i class="fas fa-pen-square fa-3x"></i></a>
                <a href="@Url.Action("Index", "Locations")" class="mx-3"><i class="fas fa-caret-square-left fa-3x"></i></a>
            </p>
        </div>
        <div class="row contact-info">

            <div class="col-md-4">

            </div>

            <div class="col-md-4">
                <strong>Address:</strong><br />
                @Html.DisplayFor(model => model.Address) <br />
                @Html.DisplayFor(model => model.State) @Html.DisplayFor(model => model.ZipCode) <br /><br />
                <strong>Reservation Limit:</strong><br />
                @Html.DisplayFor(model => model.ReservationLimit)
            </div>

            <div class="col-md-4">

            </div>

        </div>
    </div>
</section>

