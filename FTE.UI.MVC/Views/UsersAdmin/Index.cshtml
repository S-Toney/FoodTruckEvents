@model IEnumerable<IdentitySample.Models.ApplicationUser>

@{
    ViewBag.Title = "Users";
}
<section id="contact" class="section-bg">

    <div class="container" data-aos="fade-up">

        <div class="section-header">
            <h2>@ViewBag.Title</h2>
            <p class="mx-auto">
                <a href="@Url.Action("Create", "UsersAdmin")"><i class="fas fa-plus-square fa-2x pl-03"></i></a>
            </p>
        </div>
        <div class="row">
            <table class="table">
                <tr>
                    <th>
                        User Name
                    </th>
                    <th>

                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            <a href="@Url.Action("Details", "UsersAdmin", new { id = item.Id })">@Html.DisplayFor(modelItem => item.UserName)</a>
</td>
                        <td>
                            <a href="@Url.Action("Edit", "UsersAdmin", new { id = item.Id })" class="mx-3"><i class="fas fa-pen-square fa-2x"></i></a>
                            <a href="@Url.Action("Delete", "UsersAdmin", new { id = item.Id })" class="mx-3"><i class="fas fa-minus-square fa-2x"></i></a>
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</section>


